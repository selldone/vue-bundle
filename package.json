{
  "name": "@selldone/vue-bundle",
  "description": "Selldoneâ€™s app Vue project",
  "version": "0.2.7",
  "private": false,
  "license": "SEE LICENSE IN LICENSE.md",
  "author": "Selldone <info@selldone.com>",
  "homepage": "https://selldone.com/apps",
  "repository": "https://github.com/selldone/vue-bundle",
  "bugs": {
    "url": "https://github.com/selldone/vue-bundle/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": [
    "**/*.css",
    "**/*.scss",
    "**/configure.js"
  ],
  "keywords": [
    "selldone",
    "vue",
    "bundle",
    "components",
    "api"
  ],
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "npm-publish": "npm publish",
    "test": "jest --passWithNoTests",
    "codecov": "codecov"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,vue}",
      "!**/node_modules/**",
      "!**/coverage/**"
    ],
    "coverageReporters": [
      "html",
      "text-summary"
    ]
  },
  "dependencies": {
    "axios": "^0.19.2",
    "core-js": "^3.6.4",
    "register-service-worker": "^1.6.2",
    "vue": "^2.6.11",
    "vue-class-component": "^7.2.2",
    "vue-property-decorator": "^8.3.0",
    "vue-router": "^3.1.5",
    "vuetify": "^2.2.17",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "@vue/cli-plugin-babel": "^4.2.3",
    "@vue/cli-plugin-eslint": "^4.2.0",
    "@vue/cli-plugin-pwa": "^4.2.0",
    "@vue/cli-plugin-router": "^4.2.0",
    "@vue/cli-plugin-typescript": "^4.2.0",
    "@vue/cli-plugin-vuex": "^4.2.0",
    "@vue/cli-service": "^4.2.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^5.0.1",
    "@vue/test-utils": "^1.0.0-beta.32",
    "babel-jest": "^25.1.0",
    "chai": "^4.2.0",
    "codecov": "^3.6.5",
    "eslint": "^6.8.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-vue": "^6.2.2",
    "jest": "^25.1.0",
    "node-sass": "^4.12.0",
    "prettier": "^1.19.1",
    "sass": "^1.19.0",
    "sass-loader": "^8.0.2",
    "typescript": "~3.7.5",
    "vue-cli-plugin-vuetify": "^2.0.5",
    "vue-jest": "^3.0.5",
    "vue-template-compiler": "^2.6.11",
    "vuetify-loader": "^1.4.3"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "@vue/typescript/recommended",
      "@vue/prettier",
      "@vue/prettier/@typescript-eslint"
    ],
    "parserOptions": {
      "ecmaVersion": 2020
    },
    "rules": {
      "no-console": "off",
      "no-debugger": "off",
      "camelcase": "off"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
